{
  "name": "lrik1",
  "version": "0.1.1",
  "private": true,
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "jest-junit": "^5.2.0",
    "react-scripts": "2.0.3",
    "react-test-render": "^1.1.1"
  },
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 86,
        "functions": 89,
        "lines": 92,
        "statements": 92
      }
    },
    "coverageReporters": [
      "json",
      "text",
      "html",
      "lcov",
      "clover",
      "cobertura"
    ]
  },
  "jest-junit": {
    "suiteName": "App UI Jest Tests",
    "output": "./test-reports/jest/junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "usePathForSuiteName": "true"
  }
}
