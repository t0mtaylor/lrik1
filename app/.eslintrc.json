{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "extends": ["eslint:recommended"],

    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes": [2, "single"],
        "react/jsx-no-undef": 1,
        "react/jsx-uses-react": 1,
        "react/jsx-uses-vars": 1,
        "jsx-quotes": 1,
        "eol-last": 1,
        "no-use-before-define":0,
        "no-underscore-dangle":0,
        "strict": 0,
        "new-cap":0,
        "no-console":0
    }
}
